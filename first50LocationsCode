# First 50 locations that contain "Springfield" (case-insensitive)
spark.sql("""
SELECT DISTINCT raw_location_text
FROM lines_table
WHERE LOWER(raw_location_text) LIKE '%springfield%'
LIMIT 50
""").show(50, False)


